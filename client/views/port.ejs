<script>
	var images = document.getElementsByClassName("project_img");
	var fmfa = 'Worked as a Backend Developer for FMFA inc., which our team developed using MySQL, Node.js, Express.js, Angular.js, Bootstrap, some vanilla Javascript (because it is fun).';
	var ypc = "Also worked as a Backend Developer for YPC, Young Professional Council San Jose. Our team used MySQL, Node.js, Express.js, Angular.js, Bootstrap, LESS, Javascript, as well as Stripe payment API.";
	var directme = "Direct Me is a web app that uses Flask, Pylot, Python, MySQL and 4 different APIs to provide suggestions to your daily activities. These APIs are Yelp, Open Weather Map, Twilio and Google Maps."
	var captions = [`${fmfa}`, `${ypc}` ,`${directme}`];
	for (let count = 0; count < images.length; count++){
		let image = images[count];
		let size_toggle = false;
		let small_size = .28;
		image.height = small_size * window.innerHeight;
		image.width = small_size * window.innerWidth;
		image.onmouseup = function(){
			document.getElementById("caption").innerHTML = ""
			let height_per;
			let width_per;
			if (size_toggle == false){
				size_toggle = true;
				height_per = 0.8;
				width_per = 0.8;
			}
			else{
				size_toggle = false;
				height_per = small_size;
				width_per = small_size;
			}
			image.height = (height_per * window.innerHeight) + "";
			image.width = (width_per * window.innerWidth) + "";
		}
		image.onmouseover = function(){
			if (size_toggle == false){
				document.getElementById("caption").innerText = captions[count];
				// console.log(count);
		}
		}
		image.onmouseout = function(){
			document.getElementById("caption").innerText = ""
		}
	}
</script>

